<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
	  <LangVersion>10.0</LangVersion>
	  <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
	  <Platforms>AnyCPU;x64</Platforms>
	  <EnableNETAnalyzers>True</EnableNETAnalyzers>
	  <AnalysisLevel>latest-recommended</AnalysisLevel>
    <EnableWindowsTargeting>true</EnableWindowsTargeting>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="ChequeWriter.DatabaseInitializer\**" />
    <Compile Remove="Resources\**" />
    <EmbeddedResource Remove="ChequeWriter.DatabaseInitializer\**" />
    <EmbeddedResource Remove="Resources\**" />
    <None Remove="ChequeWriter.DatabaseInitializer\**" />
    <None Remove="Resources\**" />
    <Page Remove="ChequeWriter.DatabaseInitializer\**" />
    <Page Remove="Resources\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Prism.Unity" Version="8.1.97" />
  </ItemGroup>
  
  <ItemGroup>
    <ProjectReference Include="Resources\Resources.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Services\DataService\Models\" />
  </ItemGroup>

</Project>
