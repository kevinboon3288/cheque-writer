    <Folder Include="Modules\ReportModule\ViewModels\" />
    <Folder Include="Modules\ReportModule\Views\" />
    <Folder Include="Resources\Images\" />
    <Folder Include="Services\DataService\" />
  </ItemGroup>

</Project>
